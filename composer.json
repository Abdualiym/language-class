{
    "name": "infosystems/yii2-language-class",
    "description": "Yii 2 language table emulator class",
    "keywords": ["yii2", "framework", "advanced", "language"],
    "homepage": "http://www.infosystems.uz/",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ziyodilla Mirhanov",
            "email": "ziyat@infosystems.uz",
            "homepage": "http://infosystems.uz"
        }
    ],
    "support": {
        "issues": "http://git.uzcloud.uz/aripdjanov/language-class/issues",
        "source": "http://git.uzcloud.uz/aripdjanov/language-class"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "~2.0.6",
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "aripdjanov\\languageClass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "aripdjanov\\languageClass\\tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1800
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]

}
